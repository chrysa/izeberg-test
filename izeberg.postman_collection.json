                                    "const postRequest = {\r",
                                    "  url: `${host}:${port}/api/login/`,\r",
                                    "  method: 'POST',\r",
                                    "  body: {\r",
                                    "    mode: 'raw',\r",
                                    "    raw: JSON.stringify({\"username\": \"admin\",\"password\": \"admin\"})\r",
                                    "  },\r",
