                                    "  const responseData = response.json();\r",
                                    "  const authToken = responseData.token;\r",
                                    "  pm.environment.set(\"AUTH_TOKEN\", authToken);\r",
                                    "  console.log(`Successfully authenticated! Token: ${authToken}`);\r",
                                    "});\r",
                                    ""
